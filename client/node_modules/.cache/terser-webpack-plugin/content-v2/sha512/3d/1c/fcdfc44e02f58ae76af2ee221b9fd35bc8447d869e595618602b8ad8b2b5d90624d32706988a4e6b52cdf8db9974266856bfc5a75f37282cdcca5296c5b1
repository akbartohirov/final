) {\n  const auth = useAuth();\n\n  const { login, logout, token, userId } = auth;\n\n  const isAuthenticated = !!token;\n\n  const routes = useRoutes(isAuthenticated);\n\n  return (\n    <AuthContext.Provider\n      value={{ login, logout, token, userId, isAuthenticated }}\n    >\n      <Router>\n        <Navbar isAuthenticated={isAuthenticated} />\n        {routes}\n        <Footer />\n      </Router>\n    </AuthContext.Provider>\n  );\n}\n\nexport default App;\n","import React from \"react\";\nimport ReactDOM from \"react-dom\";\nimport \"./index.css\";\nimport App from \"./App\";\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById(\"root\")\n);\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin"]}}