 token } = JSON.parse(localStorage.getItem(\"userData\"));\n\n    axios\n      .delete(`/category/${id}`, {\n        headers: {\n          Authorization: `Bearer ${token}`,\n        },\n      })\n      .then(({ data }) => {\n        console.log(data);\n        window.M.toast({\n          html: \"Категория удалена!\",\n          classes: \"loginToastYellow\",\n        });\n        setCreatedCategory(data.message + Date.now());\n      });\n  };\n\n  return (\n    <div className=\"adminPage\">\n      <div id=\"modal1\" className=\"modal modal-fixed-footer\">\n        <div className=\"modal-content\">\n          <h4>Cоздать категорию</h4>\n          <form\n            onSubmit={(e) => submitHandler(e)}\n            style={{ padding: \"30px 60px\" }}\n          >\n            <div className=\"row\">\n              <div className=\"input-field col s12\">\n                <input\n                  id=\"categoryName\"\n                  type=\"text\"\n                  required\n                  className=\"validate\"\n                  value={categoryName}\n                  onChange={(e) => setCategoryName(e.target.value)}\n                />\n                <label htmlFor=\"categoryName\">Категория</label>\n              </div>\n            </div>\n\n            <div className=\"row\">\n              <div className=\"file-field input-field col s12\">\n                <div className=\"btn\">\n                  <span>Изображения</span>\n                  <input\n                    type=\"file\"\n                    required\n                    onChange={(e) => setImg(e.target.files[0])}\n                  />\n                </div>\n                <div className=\"file-path-wrapper\">\n                  <input className=\"file-path validate\" type=\"text\" />\n                </div>\n              </div>\n            </div>\n\n            <div className=\"row\">\n              <div className=\"input-field col s12\">\n                <button\n                  classNam